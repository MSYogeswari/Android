import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Enter a number");
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int num1 = num/100;
        int num2 = num%100;
        int sum = num1+num2;
        if((sum*sum) == Integer.valueOf(num))
            System.out.println("Fasion Number");
        else
            System.out.println("Not a Fasion Number");
    }
}
